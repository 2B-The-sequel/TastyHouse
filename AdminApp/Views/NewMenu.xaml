<Window x:Class="AdminApp.Views.NewMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdminApp.Views"
        mc:Ignorable="d"
        Title="NewMenu" Height="450" Width="800">
    <StackPanel>
        <DockPanel>
            <Label Content="Navn til menuen"/>
            <TextBox Name="MenuName" Width="200" HorizontalAlignment="Left" Margin="5"/>
        </DockPanel>
        <DockPanel>
            <Label Content="Indhold til menuen:"/>
            
        </DockPanel>
    </StackPanel>
</Window>
